{
  "name": "door-cloud-iot-client",
  "version": "0.1.0",
  "main": "src/index.ts",
  "description": "IoT client for DoorCloud project",
  "scripts": {
    "build:dev": "webpack --mode development",
    "build": "webpack --mode production",
    "lint": "eslint src/* --ext .ts",
    "service": "nodemon",
    "start": "node dist/index.js",
    "release": "standard-version",
    "test:local": "jest --setupFiles dotenv/config --ci -i --forceExit",
    "test:ci": "jest --ci -i"
  },
  "author": "AnthonyLzq <sluzquinosa@uni.pe>",
  "license": "MIT",
  "devDependencies": {
    "@jest/types": "^29.3.1",
    "@types/jest": "^29.2.4",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.47.1",
    "@typescript-eslint/parser": "^5.47.1",
    "dotenv": "^16.0.3",
    "eslint": "^8.30.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.1.7",
    "eslint-plugin-n": "^15.6.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.3.1",
    "jest-unit": "^0.0.2",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.1",
    "ts-jest": "^29.0.3",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.1",
    "tsconfig-paths-webpack-plugin": "^4.0.0",
    "typescript": "^4.9.4",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-node-externals": "^3.0.0"
  },
  "nodemonConfig": {
    "watch": [
      ".env",
      "src"
    ],
    "ext": "ts",
    "ignore": [
      "src/**/*.test.ts"
    ],
    "exec": "npx ts-node -r dotenv/config ./src/index"
  },
  "directories": {
    "test": "test"
  },
  "dependencies": {
    "eslint-import-resolver-node": "^0.3.6",
    "eslint-module-utils": "^2.7.4",
    "eslint-plugin-es": "^4.1.0",
    "eslint-scope": "^7.1.1",
    "eslint-utils": "^3.0.0",
    "prettier-linter-helpers": "^1.0.0",
    "eslint-visitor-keys": "^3.3.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AnthonyLzq/DoorCloud-IoTClient.git"
  },
  "keywords": [
    "iot",
    "mqtt"
  ],
  "bugs": {
    "url": "https://github.com/AnthonyLzq/DoorCloud-IoTClient/issues"
  },
  "homepage": "https://github.com/AnthonyLzq/DoorCloud-IoTClient#readme"
}
